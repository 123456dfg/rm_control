<launch>

        <rosparam file="$(find rm_base)/config/actuator_coefficient.yaml" command="load" ns="rm_base"/>
        <rosparam file="$(find rm_base)/test/test_control.yaml" command="load"/>

        <rosparam file="$(find rm_base)/test/test_control.xacro" command="load" ns="robot_description"/>

    <node name="rm_base" pkg="rm_base" type="rm_base" respawn="false"/>

    <!-- load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" args="
               controllers/joint_state_controller
               controllers/robot_state_controller

"/>



</launch>
