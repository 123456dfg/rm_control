<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="arm" params="fix">

    <!--  link  -->
    <link name="link0">
      <inertial>
        <mass value="0.293"/>
        <origin xyz="0 0 0.00608"/>
        <inertia ixx="2.916e-4" ixy="0" ixz="869.776e-9" iyy="2.9134e-4"
                 iyz="0." izz="5.507e-4"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rm_description/meshes/engineer/link0.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0.008"/>
        <geometry>
          <box size="0.12 0.12 0.016"/>
        </geometry>
      </collision>
    </link>

    <link name="link1">
      <inertial>
        <mass value="2.775151"/>
        <origin xyz="0.00286 0.00018 0.05209"/>
        <inertia ixx="1.962e-2" ixy="4573.477e-9" ixz="-3.727e-3" iyy="2.223e-2"
                 iyz="-2536.137e-9" izz="1.949e-2"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rm_description/meshes/engineer/link1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <!--TODO: add collision-->
      </collision>
    </link>

    <link name="link2">
      <inertial>
        <mass value="1.372"/>
        <origin xyz="0.07668 -0.001679 0"/>
        <inertia ixx="2.04e-3" ixy="1.959e-4" ixz="-235.828e-9" iyy="1.496e-2"
                 iyz="0." izz="1.564e-2"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rm_description/meshes/engineer/link2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.115 0 0"/>
        <geometry>
          <box size="0.335 0.100 0.094"/>
        </geometry>
      </collision>
    </link>

    <link name="link3">
      <inertial>
        <mass value="0.697"/>
