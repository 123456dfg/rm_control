    <!--  joint  -->
    <joint name="joint1" type="revolute">
      <axis xyz="0 0 1"/>
      <origin xyz="0 0 0.019"/>
      <!--      limit not work while type="continuous"-->
      <limit effort="20" velocity="999" lower="-1e16" upper="1e16"/>
      <dynamics damping="0.0" friction="0.1"/>
      <parent link="link0"/>
      <child link="link1"/>
    </joint>

    <joint name="joint2" type="revolute">
      <axis xyz="0 1 0"/>
      <origin xyz="0.11341 0 0.1380"/>
      <limit effort="60" velocity="10" lower="-1e16" upper="1e16"/>
      <dynamics damping="0.0" friction="0.2"/>
      <parent link="link1"/>
      <child link="link2"/>
    </joint>

    <joint name="joint3" type="revolute">
      <axis xyz="0 1 0"/>
      <origin xyz="0.250 0 0"/>
      <limit effort="30" velocity="10" lower="-1e16" upper="1e16"/>
      <dynamics damping="0.0" friction="0.1"/>
      <parent link="link2"/>
      <child link="link3"/>
    </joint>

    <joint name="joint4" type="revolute">
      <axis xyz="0 1 0"/>
      <origin xyz="0.180 0 0"/>
      <limit effort="1.2" velocity="50" lower="-1e16" upper="1e16"/>
      <dynamics damping="0.0" friction="0.05"/>
      <parent link="link3"/>
      <child link="link4"/>
    </joint>

    <joint name="joint5" type="revolute">
      <axis xyz="1 0 0"/>
      <origin xyz="0 0 0"/>
      <limit effort="1.2" velocity="50" lower="-1e16" upper="1e16"/>
      <dynamics damping="0.0" friction="0.05"/>
      <parent link="link4"/>
      <child link="link5"/>
