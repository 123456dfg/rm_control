      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.0513 0.0882 0.0513"/>
        </geometry>
      </collision>
    </link>

    <link name="link5">
      <inertial>
        <mass value="0.257"/>
        <origin xyz="0.02803 0.00084 -0.00089"/>
        <inertia ixx="6.485e-5" ixy="-754.967e-9" ixz="-793.024e-9" iyy="2.477e-4"
