      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rm_description/meshes/engineer/link3.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.100 0 0"/>
        <geometry>
          <box size="0.2488 0.089 0.0895"/>
        </geometry>
      </collision>
    </link>

    <link name="link4">
      <inertial>
        <mass value="0.367"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="2.803e-4" ixy="0.15e-9" ixz="0" iyy="19.296e-5"
                 iyz="0." izz="2.798e-4"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rm_description/meshes/engineer/link4.stl" scale="0.001 0.001 0.001"/>
